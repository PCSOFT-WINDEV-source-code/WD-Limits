     code : |1-
      // Syntax:
      //UpdStcDuration ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      PROCEDURE UpdStcDuration()
      
      
      nbDays is int
      	
      // Is the application limited
      nbDays = pDurationLimit.IsLimited()
      
      // Make the control for remaining time invisible
      STC_REMAINDER..Visible = False
      
      
      SWITCH nbDays
      
      	CASE 0
      		//Fraud
      		STC_ENABLED = <ยง$0002ยง>
      		
      	CASE -1
      		
      		STC_ENABLED = <ยง$0003ยง>
      		
      	OTHER CASE 
      		
      		STC_ENABLED = <ยง$0004ยง>
      		STC_REMAINDER = <ยง$0005ยง> + nbDays + <ยง$0006ยง>
      		STC_REMAINDER..Visible = True
      		
      END
     type : 458752
